<Window x:Class="CRAPI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CRAPI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel LastChildFill="True">
    <StackPanel Orientation="Vertical">
      <Button Content="Mute" x:Name="btnMute" Click="btnMute_Click"></Button>
      <Button Content="Un Mute" x:Name="btnUnMute" Click="btnUnMute_Click"></Button>
    </StackPanel>
    <ListBox ItemsSource="{Binding Log}"  x:Name="logBox">
    </ListBox>
    </DockPanel>
</Window>
